<div class="container">
  <!-- <h1>All posts:</h1> -->
  <% @posts.each do |post| %>
    <tr>
      <td>
        <div class="post">
          <%= link_to post.title, post_path(post) %>
          <%= post.date.strftime("%d %b %Y") %>
        </div>
      </td>
    </tr>
  <% end %>
  <div class="back-link">
    <%= link_to 'New Post', new_post_path %>
  </div>
</div>
