<%= form_for (@post) do |f| %>
  <%= render 'shared/error_messages' %>

  <%= f.text_field :title,
  placeholder: 'Type the post title here',
  lable: true %>
  <%= f.date_select :date %>
  <%= f.cktext_area :content,
  class: 'ckeditor'
  %>
  <%= f.submit %>
<% end %>
<%= javascript_include_tag Ckeditor.cdn_url %>
